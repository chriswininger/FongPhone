{
  "name": "FongPhone",
  "description": "A mobile app for live audio performance and all around weirdness",
  "version": "1.1.0",
  "devDependencies": {
    "async": "1.4.x",
    "bower": "^1.8.4",
    "express": "4.13.1",
    "gulp": "^3.9.1",
    "lodash": "3.10.x",
    "recursive-readdir": "1.2.x",
    "save-svg-as-png": "1.0.2",
    "cordova-plugin-whitelist": "1",
    "cordova-plugin-crosswalk-webview": "~2.3.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-splashscreen": {}
    },
    "platforms": []
  },
  "dependencies": {
    "cordova-android": "^6.2.2"
  }
}
